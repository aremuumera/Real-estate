{
    "version": 2,
    "builds": [
      {
        "src": "api/*",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "50mb"
        },
        "buildCommand": "npm install && npx prisma generate && npm run build"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/api"
      }
    ]
  }
  